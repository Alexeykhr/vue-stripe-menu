{
  "name": "vue-stripe-menu",
  "version": "1.2.5",
  "description": "Creating menus with animations like on Stripe",
  "main": "./dist/vue-stripe-menu.common.js",
  "scripts": {
    "dev": "vue-cli-service serve ./demo/main.js",
    "build": "vue-cli-service build --dest docs ./demo/main.js",
    "build:lib": "vue-cli-service build --target lib --name vue-stripe-menu ./src/main.js",
    "lint": "vue-cli-service lint src demo --no-fix",
    "lint:fix": "vue-cli-service lint src demo",
    "test": "jest",
    "rel": "npm run lint:fix && npm run test && npm run build && npm run build:lib && git add -A && standard-version -a",
    "pub": "git push --follow-tags origin dev && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alexeykhr/vue-stripe-menu.git"
  },
  "author": "Alexey Khrushch, alexeykhr@outlook.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alexeykhr/vue-stripe-menu/issues"
  },
  "homepage": "https://github.com/Alexeykhr/vue-stripe-menu#readme",
  "dependencies": {
    "core-js": "^3.6.3",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "@vue/babel-preset-app": "^4.1.2",
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "@vue/eslint-config-standard": "^5.1.0",
    "@vue/test-utils": "^1.0.0-beta.30",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.1.2",
    "highlight.js": "^9.17.1",
    "jest": "^25.1.0",
    "node-sass": "^4.13.0",
    "raw-loader": "^4.0.0",
    "sass-loader": "^8.0.1",
    "sinon": "^8.0.4",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11"
  }
}
